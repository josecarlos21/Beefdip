{
  "animation": {
    "visual-pulse": { "$value": "vpulse 3s ease-in-out infinite", "$type": "cubic-bezier" },
    "reveal": { "$value": "reveal 0.6s cubic-bezier(0.34, 1.56, 0.64, 1) forwards", "$type": "cubic-bezier" },
    "float": { "$value": "float 4s ease-in-out infinite", "$type": "cubic-bezier" },
    "connector-flow": { "$value": "flow 3s linear infinite", "$type": "cubic-bezier" }
  },
  "keyframes": {
    "vpulse": {
      "0%, 100%": { "$value": { "opacity": "0.4", "transform": "scale(1)" }, "$type": "unknown" },
      "50%": { "$value": { "opacity": "0.8", "transform": "scale(1.05)" }, "$type": "unknown" }
    },
    "reveal": {
      "0%": { "$value": { "opacity": "0", "transform": "translateY(24px)" }, "$type": "unknown" },
      "100%": { "$value": { "opacity": "1", "transform": "translateY(0)" }, "$type": "unknown" }
    },
    "float": {
      "0%, 100%": { "$value": { "transform": "translateY(0)" }, "$type": "unknown" },
      "50%": { "$value": { "transform": "translateY(-10px)" }, "$type": "unknown" }
    },
    "flow": {
      "0%": { "$value": { "strokeDashoffset": "40" }, "$type": "unknown" },
      "100%": { "$value": { "strokeDashoffset": "0" }, "$type": "unknown" }
    }
  }
}
