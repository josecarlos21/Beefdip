{
  "animation": {
    "visual-pulse": { "$value": "vpulse 3s ease-in-out infinite", "$type": "transition" },
    "reveal": { "$value": "reveal 0.6s cubic-bezier(0.34, 1.56, 0.64, 1) forwards", "$type": "transition" },
    "float": { "$value": "float 4s ease-in-out infinite", "$type": "transition" },
    "connector-flow": { "$value": "flow 3s linear infinite", "$type": "transition" }
  },
  "keyframes": {
    "vpulse": {
      "0%, 100%": { "opacity": "0.4", "transform": "scale(1)" },
      "50%": { "opacity": "0.8", "transform": "scale(1.05)" }
    },
    "reveal": {
      "0%": { "opacity": "0", "transform": "translateY(24px)" },
      "100%": { "opacity": "1", "transform": "translateY(0)" }
    },
    "float": {
      "0%, 100%": { "transform": "translateY(0)" },
      "50%": { "transform": "translateY(-10px)" }
    },
    "flow": {
      "0%": { "strokeDashoffset": "40" },
      "100%": { "strokeDashoffset": "0" }
    }
  }
}
