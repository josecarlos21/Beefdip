{
  "animation": {
    "duration": {
      "short": { "$value": "0.2s", "$type": "duration" },
      "medium": { "$value": "0.6s", "$type": "duration" },
      "long": { "$value": "3s", "$type": "duration" }
    },
    "easing": {
      "inOut": { "$value": "ease-in-out", "$type": "cubicBezier" },
      "reveal": { "$value": "cubic-bezier(0.34, 1.56, 0.64, 1)", "$type": "cubicBezier" }
    },
    "custom": {
      "visualPulse": { "$value": "vpulse 3s ease-in-out infinite", "$type": "transition" },
      "reveal": { "$value": "reveal 0.6s cubic-bezier(0.34, 1.56, 0.64, 1) forwards", "$type": "transition" },
      "float": { "$value": "float 4s ease-in-out infinite", "$type": "transition" },
      "connectorFlow": { "$value": "flow 3s linear infinite", "$type": "transition" }
    },
    "keyframes": {
      "vpulse": {
        "$value": {
          "0%, 100%": { "opacity": "0.4", "transform": "scale(1)" },
          "50%": { "opacity": "0.8", "transform": "scale(1.05)" }
        },
        "$type": "keyframes"
      },
      "reveal": {
        "$value": {
          "0%": { "opacity": "0", "transform": "translateY(24px)" },
          "100%": { "opacity": "1", "transform": "translateY(0)" }
        },
        "$type": "keyframes"
      },
      "float": {
        "$value": {
          "0%, 100%": { "transform": "translateY(0)" },
          "50%": { "transform": "translateY(-10px)" }
        },
        "$type": "keyframes"
      },
      "flow": {
        "$value": {
          "0%": { "strokeDashoffset": "40" },
          "100%": { "strokeDashoffset": "0" }
        },
        "$type": "keyframes"
      }
    }
  }
}
