{
  "animation": {
    "keyframes": {
      "vpulse": {
        "$value": {
          "0%, 100%": { "opacity": "0.4", "transform": "scale(1)" },
          "50%": { "opacity": "0.8", "transform": "scale(1.05)" }
        },
        "$type": "keyframes"
      },
      "reveal": {
        "$value": {
          "0%": { "opacity": "0", "transform": "translateY(24px)" },
          "100%": { "opacity": "1", "transform": "translateY(0)" }
        },
        "$type": "keyframes"
      },
      "float": {
        "$value": {
          "0%, 100%": { "transform": "translateY(0)" },
          "50%": { "transform": "translateY(-10px)" }
        },
        "$type": "keyframes"
      },
      "flow": {
        "$value": {
          "0%": { "strokeDashoffset": "40" },
          "100%": { "strokeDashoffset": "0" }
        },
        "$type": "keyframes"
      }
    },
    "duration": {
        "fast": { "$value": "0.2s", "$type": "duration" },
        "normal": { "$value": "0.6s", "$type": "duration" },
        "slow": { "$value": "3s", "$type": "duration" },
        "slower": { "$value": "4s", "$type": "duration" }
    },
    "ease": {
        "in-out": { "$value": "ease-in-out", "$type": "cubic-bezier" },
        "reveal": { "$value": "cubic-bezier(0.34, 1.56, 0.64, 1)", "$type": "cubic-bezier" },
        "linear": { "$value": "linear", "$type": "cubic-bezier" }
    }
  }
}
