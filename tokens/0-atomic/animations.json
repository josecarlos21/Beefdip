{
  "animations": {
    "visual-pulse": { "$value": "vpulse 3s ease-in-out infinite", "$type": "string" },
    "reveal": { "$value": "reveal 0.6s cubic-bezier(0.34, 1.56, 0.64, 1) forwards", "$type": "string" },
    "float": { "$value": "float 4s ease-in-out infinite", "$type": "string" },
    "connector-flow": { "$value": "flow 3s linear infinite", "$type": "string" }
  },
  "keyframes": {
    "vpulse": {
      "$value": {
        "0%, 100%": { "opacity": "0.4", "transform": "scale(1)" },
        "50%": { "opacity": "0.8", "transform": "scale(1.05)" }
      },
      "$type": "keyframes"
    },
    "reveal": {
      "$value": {
        "0%": { "opacity": "0", "transform": "translateY(24px)" },
        "100%": { "opacity": "1", "transform": "translateY(0)" }
      },
      "$type": "keyframes"
    },
    "float": {
      "$value": {
        "0%, 100%": { "transform": "translateY(0)" },
        "50%": { "transform": "translateY(-10px)" }
      },
      "$type": "keyframes"
    },
    "flow": {
      "$value": {
        "0%": { "strokeDashoffset": "40" },
        "100%": { "strokeDashoffset": "0" }
      },
      "$type": "keyframes"
    }
  }
}
