{
  "animation": {
    "visual-pulse": { "$value": "vpulse 3s ease-in-out infinite", "$type": "cubic-bezier" },
    "reveal": { "$value": "reveal 0.6s cubic-bezier(0.34, 1.56, 0.64, 1) forwards", "$type": "cubic-bezier" },
    "float": { "$value": "float 4s ease-in-out infinite", "$type": "cubic-bezier" },
    "connector-flow": { "$value": "flow 3s linear infinite", "$type": "cubic-bezier" }
  },
  "keyframes": {
     "vpulse": {
         "$value": {
             "0%, 100%": { "opacity": "0.4", "transform": "scale(1)" },
             "50%": { "opacity": "0.8", "transform": "scale(1.05)" }
         },
         "$type": "unknown"
     },
     "reveal": {
         "$value": {
             "0%": { "opacity": "0", "transform": "translateY(24px)" },
             "100%": { "opacity": "1", "transform": "translateY(0)" }
         },
         "$type": "unknown"
     },
     "float": {
         "$value": {
             "0%, 100%": { "transform": "translateY(0)" },
             "50%": { "transform": "translateY(-10px)" }
         },
         "$type": "unknown"
     },
     "flow": {
         "$value": {
             "0%": { "strokeDashoffset": "40" },
             "100%": { "strokeDashoffset": "0" }
         },
         "$type": "unknown"
     }
  }
}
