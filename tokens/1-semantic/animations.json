{
  "animation": {
    "semantic": {
      "visual-pulse": { "$value": "vpulse {animation.timing.normal} {animation.curve.ease_in_out} infinite", "$type": "string" },
      "reveal": { "$value": "reveal {animation.timing.fast} {animation.curve.reveal_curve} forwards", "$type": "string" },
      "float": { "$value": "float {animation.timing.slow} {animation.curve.ease_in_out} infinite", "$type": "string" },
      "connector-flow": { "$value": "flow {animation.timing.normal} {animation.curve.linear} infinite", "$type": "string" }
    }
  }
}
